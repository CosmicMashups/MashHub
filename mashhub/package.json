{
  "name": "mashhub",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "homepage": "https://github.com/CosmicMashups/MashHub",
  "scripts": {
    "dev": "vite",
    "typecheck": "tsc -b --noEmit",
    "build": "vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "hook-safety:check": "eslint src --ext ts,tsx --max-warnings 0 --rule \"react-hooks/rules-of-hooks:error\"",
    "preview": "vite preview",
    "tailwindcss": "tailwindcss -i ./src/index.css -o ./dist/output.css --watch",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:e2e": "playwright test",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@tanstack/react-table": "^8.21.3",
    "colorthief": "^2.6.0",
    "dexie": "^4.2.0",
    "exceljs": "^4.4.0",
    "framer-motion": "^12.23.16",
    "fuse.js": "^7.1.0",
    "lucide-react": "^0.544.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "@playwright/test": "^1.55.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/exceljs": "^0.5.3",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@types/xlsx": "^0.0.35",
    "@vitejs/plugin-react": "^5.0.2",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.35.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "fake-indexeddb": "^6.2.5",
    "gh-pages": "^6.3.0",
    "globals": "^16.4.0",
    "jsdom": "^27.0.0",
    "playwright": "^1.55.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.43.0",
    "vite": "^7.1.6",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-pwa": "^1.2.0",
    "vitest": "^3.2.4"
  }
}
